    "version": "0.2.0",
    "configurations": [
        {
			"cwd": "${workspaceFolder}/example",
			"command": "npm run start",
			"name": "Example debug prod",
			"request": "launch",
			"type": "node-terminal",
			"serverReadyAction": {
				"pattern": "Server start on port ([0-9]+)",
				"uriFormat": "https://localhost:%s",
				"action": "openExternally"
			}
		},
		{
			"cwd": "${workspaceFolder}/example",
			"command": "npm run start-dev",
			"name": "Example debug dev",
			"request": "launch",
			"type": "node-terminal",
			"serverReadyAction": {
				"pattern": "Server start on port ([0-9]+)",
				"uriFormat": "https://localhost:%s",
				"action": "openExternally"
			}
		}
    ]
}